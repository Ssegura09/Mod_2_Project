<%= form_with model: @traveler, :url => url_for(:controller => 'travelers', :action => 'create') do | f |%>

<%= f.label :email%>
<%= f.text_field :email %>

<%= f.label :password %>
<%= f.password_field :password %>

<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>

<%= f.label :first_name %>
<%= f.text_field :first_name %>

<%= f.label :last_name %>
<%= f.text_field :last_name %>

<%= f.label :age %>
<%= f.text_field :age %>

<%= f.submit %>
<% end %>


